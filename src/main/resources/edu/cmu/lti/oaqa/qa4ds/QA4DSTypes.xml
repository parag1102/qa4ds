<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>QA4DSTypes</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <imports>
    <import name="edu.cmu.lti.oaqa.OAQATypes"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.lti.oaqa.qa4ds.types.DecisionTemplate</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>vars</name>
          <description/>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      <featureDescription>
          <name>question</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>factors</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>edu.cmu.lti.oaqa.qa4ds.types.WeightedDecisionTemplate</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>primitive</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.lti.oaqa.qa4ds.types.WeightDecisionTemplate</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>template</name>
          <description/>
          <rangeTypeName>edu.cmu.lti.oaqa.qa4ds.types.DecisionTemplate</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>weight</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.lti.oaqa.qa4ds.types.DecisionTemplateInput</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>template</name>
          <description/>
          <rangeTypeName>edu.cmu.lti.oaqa.qa4ds.types.DecisionTemplate</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>values</name>
          <description/>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
